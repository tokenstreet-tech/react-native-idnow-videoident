{
    "name": "@tokenstreet/react-native-idnow-videoident",
    "version": "0.20.1",
    "description": "ðŸ“¹ React Native IDnow VideoIdent bindings for the iOS and Android SDK",
    "main": "lib/commonjs/index",
    "module": "lib/module/index",
    "types": "lib/typescript/index.d.ts",
    "react-native": "src/index",
    "source": "src/index",
    "files": [
        "src",
        "lib",
        "android",
        "ios",
        "cpp",
        "*.podspec",
        "!lib/typescript/example",
        "!ios/build",
        "!android/build",
        "!android/gradle",
        "!android/gradlew",
        "!android/gradlew.bat",
        "!android/local.properties",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "!**/.*"
    ],
    "scripts": {
        "xcode": "open ios/ReactNativeIdnowVideoident.xcodeproj",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "prettier --write . --ignore-path .gitignore",
        "uncrustify": "cd ios && find . \\( -name \"*.h\" -o -name \"*.m\" \\) -exec uncrustify -c ../uncrustifyrc.cfg --no-backup {} +",
        "build": "bob build",
        "pods": "cd examples/bare/ios && LC_ALL=en_US.UTF-8 NO_FLIPPER=1 pod update",
        "examples": "yarn --cwd examples/bare && yarn pods && yarn --cwd examples/expo",
        "website": "yarn --cwd website",
        "bootstrap": "yarn && yarn website && yarn build && yarn examples",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*": "prettier --ignore-unknown --write",
        "*.(js|jsx|ts|tsx)": "eslint --fix"
    },
    "keywords": [
        "react-native",
        "ios",
        "android"
    ],
    "repository": "https://github.com/tokenstreet-tech/react-native-idnow-videoident",
    "author": "Daniel Reichhart <daniel@tokenstreet.com> (https://tokenstreet.com/)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tokenstreet-tech/react-native-idnow-videoident/issues"
    },
    "homepage": "https://tokenstreet-tech.github.io/react-native-idnow-videoident/",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "dependencies": {},
    "devDependencies": {
        "@commitlint/config-conventional": "^17.4.4",
        "@prettier/plugin-ruby": "2.1.0",
        "@prettier/plugin-xml": "2.2.0",
        "@react-native-community/eslint-config": "^3.2.0",
        "@types/jest": "^29.4.0",
        "@types/react": "^18.0.28",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "commitlint": "^17.4.4",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-native": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "expo": "^48.0.5",
        "husky": "^8.0.3",
        "jest": "^29.4.3",
        "lint-staged": "^13.1.2",
        "prettier": "2.8.5",
        "prettier-plugin-java": "2.1.0",
        "react": "18.2.0",
        "react-native": "0.71.4",
        "react-native-builder-bob": "^0.20.4",
        "ts-node": "^10.9.1",
        "typescript": "~5.0.2"
    },
    "resolutions": {
        "@types/react": "^18"
    },
    "peerDependencies": {
        "expo": ">=46.0.0",
        "react": "*",
        "react-native": "*"
    },
    "peerDependenciesMeta": {
        "expo": {
            "optional": true
        }
    },
    "packageManager": "^yarn@1.22.15",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    }
}
